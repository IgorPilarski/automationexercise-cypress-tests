{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["ES6", "DOM"],
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress"],
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["cypress/**/*.ts", "cypress/support"]
  }